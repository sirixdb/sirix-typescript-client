{
    "compilerOptions": {
        "declaration": true,
        "lib": [ "es2015", "DOM" ],
        "module": "ES6",
        "moduleResolution": "node",
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "target": "es5",
        "esModuleInterop": true,
        "outDir": "./dist"
    },
    "exclude": [
        "dist",
        "__tests__",
        "resources"
    ]
}